{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run tw:build && tsc && vite build && npm run tw:clean",
    "preview": "vite preview",
    "tw:watch": "tailwindcss -i ./src/tailwind-input.css -o ./src/tailwind.css --watch",
    "tw:build": "tailwindcss -i ./src/tailwind-input.css -o ./public/tailwind.css --minify",
    "tw:clean": "rm ./src/tailwind.css ./public/tailwind.css"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.3",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "framer-motion": "^7.1.0",
    "luxon": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.0.8",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@types/luxon": "^3.0.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "tailwindcss": "^3.1.7",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  }
}
